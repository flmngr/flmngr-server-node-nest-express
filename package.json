{
  "name": "@flmngr/flmngr-server-node-nest-express",
  "description": "Include Flmngr file manager server-side into your Nest (Express-based) website or app",
  "version": "1.5.2",
  "private": false,
  "author": {
    "name": "EdSDK",
    "url": "https://edsdk.com/"
  },
  "homepage": "https://flmngr.com",
  "repository": "https://github.com/flmngr/flmngr-server-node-nest-express",
  "license": "LGPL v3",
  "scripts": {
    "clean": "rm -Rf dist",
    "build": "npm run clean && tsc -b ./tsconfig.json ./tsconfig.esm.json"
  },
  "type": "commonjs",
  "dependencies": {
    "@flmngr/flmngr-server-node-express": "^1.5.2",
    "@flmngr/flmngr-server-node": "^1.5.2",
    "@nestjs/common": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "18.16.12",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "file-manager",
    "fileManager",
    "file",
    "image",
    "manager",
    "server",
    "express",
    "node"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "engines" : {
    "node" : ">=14.15.0"
  },
  "types": "./dist/cjs/index.d.ts",
  "main": "./dist/cjs/index.js"
}
